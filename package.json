{
  "name": "tembiapo-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "initiate-prisma-reset": "cd packages/db && npx prisma generate && npx prisma db push --force-reset && npx tsx prisma/seeds/initial-seed.ts",
    "prisma:generate": "cd packages/db && npx prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:back": "cd apps/back-end && nest start --watch",
    "build:back": "cd apps/back-end && nest build",
    "build:front": "cd apps/front-end && pnpm run build",
    "start:back": "cd apps/back-end && pnpm start:prod",
    "start:front": "cd apps/front-end && pnpm start",
    "install:back": "cd apps/back-end && pnpm install",
    "dev:front": "cd apps/front-end && pnpm run dev",
    "install:front": "cd apps/front-end && pnpm install",
    "install:types": "cd packages/types && pnpm install",
    "install:all": "pnpm install",
    "setup:project": "pnpm install && pnpm run db:setup",
    "db:setup": "pnpm --filter @tembiapo/db run setup",
    "dev": "pnpm run --recursive --parallel dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@types/node": "^24.10.0",
    "eslint": "^9.39.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
